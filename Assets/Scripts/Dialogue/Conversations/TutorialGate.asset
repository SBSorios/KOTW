  speakerLeft: {fileID: 11400000, guid: dcca46c76871ea84281a814037312377, type: 2}
  speakerRight: {fileID: 11400000, guid: 395a864087c77fb4289236030a7dcc48, type: 2}
  lines:
  - character: {fileID: 11400000, guid: 395a864087c77fb4289236030a7dcc48, type: 2}
    text: 'Can you believe we really thought these flimsy gates could keep a titan
      in? '
  - character: {fileID: 11400000, guid: 395a864087c77fb4289236030a7dcc48, type: 2}
    text: You'll see them all over as failed doors. How come we never learn from
      past mistakes?
  - character: {fileID: 11400000, guid: 395a864087c77fb4289236030a7dcc48, type: 2}
    text: You can lift the gate if you activate the green buttons at the top with
      your Wind Brush.
  - character: {fileID: 11400000, guid: 395a864087c77fb4289236030a7dcc48, type: 2}
    text: Don't worry about shutting this one behind you, it won't stop the big bad!
